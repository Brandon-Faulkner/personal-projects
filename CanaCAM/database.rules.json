{
  "rules": {
    "Groups":{
      ".read": "auth != null",
      ".write": "auth != null && auth.token.email.endsWith('@canachurch.com')"
    },
    "GroupsInfo": {
      ".read": "auth != null && auth.token.firebase.sign_in_provider != 'anonymous'",
      ".write": "auth != null && auth.token.email.endsWith('@canachurch.com')" 
    },
    "Users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }    
  }
}